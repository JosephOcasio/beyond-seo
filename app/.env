APP_DEBUG=true
RC_AUT_KEY=...
SYMFONY_DEPRECATIONS_HELPER=weak

DB_DEFAULT_CONNECTION_DRIVER='pdo_mysql'
DB_DEFAULT_CONNECTION_HOST="%DB_HOST%"
DB_DEFAULT_CONNECTION_USER="%DB_USER%"
DB_DEFAULT_CONNECTION_PASSWORD="%DB_PASSWORD%"
DB_DEFAULT_CONNECTION_DB_NAME="%DB_NAME%"
DB_DEFAULT_CONNECTION_DB_SERVER_CHARSET='utf8mb4'
DB_DEFAULT_DOCTRINE_PROXY_GENERATION_MODE='AUTOGENERATE_FILE_NOT_EXISTS'

CACHE_DEFAULT_CACHE_GROUP='phpfiles'
CACHE_DOCTRINE_CACHE_GROUP='phpfiles'
CACHE_DOCTRINE_METADATA_CACHE_GROUP='phpfiles'
CACHE_DEFAULT_TTL=300

CACHE_PHPFILES_CACHE_GROUP=phpfiles
CACHE_PHPFILES_NAMESPACE="%APP_PREFIX%_rankingCoach"
CACHE_PHPFILES_TTL=300
CACHE_PHPFILES_DIRECTORY="%APP_ROOT_DIR%/Cache"

# Lazyload
LAZYLOAD_DEFAULT_REPO_TYPE='INTERNAL_DB'
